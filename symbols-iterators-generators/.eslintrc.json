{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-param-reassign":"off",
      "import/extensions": "off",
      "no-console":"off",
      "func-names":"off",
      "no-plusplus":"off",
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    }
  }