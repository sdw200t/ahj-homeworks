{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "import/extensions": "off",
    "no-console":"off",
    "no-unused-vars":"off",
    "no-plusplus":"off",
    "no-param-reassign":"off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}